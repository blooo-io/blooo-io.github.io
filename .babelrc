{
  "presets": ["@babel/env", "@babel/react", "@babel/preset-typescript"],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    ["@babel/plugin-proposal-class-properties"],
    [
      "babel-plugin-rewrite-require",
      {
        "aliases": {
          "crypto": "crypto-browserify"
        }
      }
    ]
  ]
}
